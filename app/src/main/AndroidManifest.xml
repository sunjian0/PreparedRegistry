<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.preparedregistry">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <android:uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <android:uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.NoticeActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.RegisterFatherActivity" />
        <activity android:name=".activity.RegisterMotherActivity" />
        <activity android:name=".activity.ResultCheckActivity" />
        <activity android:name=".activity.RecheckRequest" />
        <activity android:name=".activity.SchoolInfor" />
        <activity android:name=".activity.Faq"></activity>
    </application>

</manifest>